<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/screen/window.xsd"
caption="msg://editorCaption"
focusComponent="fieldGroup"
messagesPack="com.cubaplatform.petclinic.web.vet.specialty">
<data>
    <instance id="specialtyCt"
      class="com.cubaplatform.petclinic.entity.vet.Specialty"
      view="_local">
        <loader/>
    </instance>

</data>

<dialogMode height="600"
  width="800"/>

<layout expand="editorActions" spacing="true">
    <form id="fieldGroup" dataContainer="specialtyCt">
        <column width="250px">
            <textField property="name"/>
        </column>
    </form>

    <fragment id="editorActions"
      screen="editorActions"/>
</layout>
</window>
